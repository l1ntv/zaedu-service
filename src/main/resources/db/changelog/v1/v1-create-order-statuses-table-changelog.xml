<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="v1-create-order_statuses" author="Vadim Duz">
        <createTable tableName="order_statuses">
            <column name="status_id" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="varchar(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="v1-insert-data-order_statuses" author="YourName">
        <preConditions>
            <not>
                <tableIsEmpty tableName="order_statuses"/>
            </not>
        </preConditions>
        <loadData tableName="order_statuses" file="db/data/order-statuses-data.csv"/>
    </changeSet>
</databaseChangeLog>